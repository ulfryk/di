{
  "name": "@samwise-tech/di",
  "version": "1.0.0",
  "description": "A Simple DI library",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "rm -rf ./lib && node_modules/.bin/tsc --project tsconfig.publish.json",
    "karma": "node_modules/.bin/karma start config/karma.conf.js",
    "lint": "node_modules/.bin/tslint --project tsconfig.tslint.json --format verbose './src/**/*.{ts,tsx}'",
    "mocha": "node_modules/.bin/mocha --opts ./mocha.opts './src/**/*.spec.ts'",
    "prepublish": "yarn build",
    "test": "yarn outdated && yarn lint && yarn test-specs-for-only && yarn mocha && yarn karma",
    "test-specs-for-only": "node_modules/.bin/forbidden 'src/**/*.spec.ts' -p describe.only it.only"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/samwise-tech/di.git"
  },
  "keywords": [
    "di",
    "ts",
    "typescript",
    "dependency injection"
  ],
  "author": "Samwise Technology, Inc. <icunningham@samwise.tech>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/samwise-tech/di/issues"
  },
  "homepage": "https://github.com/samwise-tech/di#readme",
  "engines": {
    "node": ">=6"
  },
  "devDependencies": {
    "@samwise-tech/forbidden-patterns": "1.0.0",
    "@samwise-tech/tslint-config": "1.0.1",
    "@types/chai": "4.0.10",
    "@types/mocha": "2.2.45",
    "@types/node": "8.5.2",
    "chai": "4.1.2",
    "circular-dependency-plugin": "4.3.0",
    "core-js": "2.5.3",
    "karma": "2.0.0",
    "karma-chai": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-mocha-reporter": "2.2.5",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "2.0.9",
    "mocha": "4.0.1",
    "monet": "0.9.0-alpha.4",
    "reflect-metadata": "^0.1.10",
    "ts-loader": "3.2.0",
    "ts-node": "4.1.0",
    "tslint": "5.8.0",
    "typescript": "2.6.2",
    "webpack": "3.10.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.1.10"
  }
}
